{
  "name": "ngx-collection",
  "version": "4.2.0",
  "license": "MIT",
  "author": {
    "name": "Evgeniy OZ",
    "email": "normandiggs@gmail.com",
    "url": "https://jamm.dev"
  },
  "repository": {
    "url": "git+https://github.com/e-oz/ngx-collection.git"
  },
  "keywords": [
    "angular",
    "collections"
  ],
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "npx jest",
    "pub": "npm run pre && npm publish ./dist/ngx-collection",
    "pre": "npm i && ng build && cp ./README.md ./dist/ngx-collection/README.md && npx jest --clearCache && npm run test && npm publish ./dist/ngx-collection --dry-run",
    "reinst": "rm -rf node_modules && rm -f package-lock.json && rm -rf dist && npm i && npm run pre"
  },
  "private": false,
  "dependencies": {
    "@angular/core": "^19.0.0-next.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0-next.2",
    "@angular-devkit/core": "^19.0.0-next.2",
    "@angular-devkit/schematics": "^19.0.0-next.2",
    "@angular/cli": "^19.0.0-next.2",
    "@angular/common": "^19.0.0-next.2",
    "@angular/compiler": "^19.0.0-next.2",
    "@angular/compiler-cli": "^19.0.0-next.2",
    "@angular/platform-browser": "^19.0.0-next.2",
    "@angular/platform-browser-dynamic": "^19.0.0-next.2",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.2",
    "ng-packagr": "^18.2.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.1-rc",
    "zone.js": "~0.15.0"
  },
  "overrides": {
    "rxjs": "$rxjs",
    "typescript": "$typescript"
  },
  "sideEffects": false
}
